{
    "routes": [
        {
            "src": "/maintenance.html",
            "dest": "/maintenance.html"
        },
        {
            "src": "/(.*\\.(?:js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|json|webp|mp4|mov|avi|m4a|mp3|ogg|txt|pdf))",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-maintenance",
                    "value": "on"
                }
            ],
            "dest": "/maintenance.html"
        }
    ]
}